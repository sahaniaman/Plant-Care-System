{
  "version": 2,
  "projects": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    },
    {
      "src": "backend/app.py",
      "use": "@vercel/python",
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "backend/app.py"
        }
      ]
    }
  ]
}
